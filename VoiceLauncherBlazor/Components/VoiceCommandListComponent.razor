@if (Show)
{

	@foreach (var listName in ListNames)
	{
		@if (!string.IsNullOrEmpty(listName) && CommandSet.SpeechLists.Where(l => l.Name == listName).Any())
		{
			<table class="table table-responsive-sm table-dark table-bordered">
				<tr>
					<th>
						@listName
					</th>
				</tr>
				@foreach (var listValue in CommandSet.SpeechLists.Where(v => v.Name == listName).FirstOrDefault()?.ListValues)
				{
					<tr>
						<td>
							@listValue?.Value_Text
						</td>
					</tr>
				}
			</table>
		}
	}
}