{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v1.3.19/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "connection-string": "Data Source=Localhost;Initial Catalog=VoiceLauncher;Integrated Security=True;Connect Timeout=120;Encrypt=False;TrustServerCertificate=True;ApplicationIntent=ReadWrite;MultiSubnetFailover=False",
    "options": {
      "set-session-context": false
    }
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/api",
      "request-body-strict": true
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "production"
    }
  },
  "entities": {
    "Languages": {
      "source": {
        "object": "dbo.Languages",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Languages",
          "plural": "Languages"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "Categories": {
      "source": {
        "object": "dbo.Categories",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Categories",
          "plural": "Categories"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "CustomIntelliSense": {
      "source": {
        "object": "dbo.CustomIntelliSense",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "CustomIntelliSense",
          "plural": "CustomIntelliSenses"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    }
  }
}