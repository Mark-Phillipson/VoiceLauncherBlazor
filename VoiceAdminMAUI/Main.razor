<CascadingBlazoredModal Class="blazored-modal-custom">



<Router AppAssembly="@typeof(VoiceAdminMAUI.Main).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(VoiceAdminMAUI.Shared.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
    <LayoutView Layout="@typeof(VoiceAdminMAUI.Shared.MainLayout)">
            <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
  </CascadingBlazoredModal>
